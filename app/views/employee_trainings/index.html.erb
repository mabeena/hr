<h1>Listing employee_trainings</h1>

<table>
  <thead>
    <tr>
      <th>Employee</th>
      <th>Training</th>
      <th>Tdate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_trainings.each do |employee_training| %>
      <tr>
        <td><%= employee_training.employee_id %></td>
        <td><%= employee_training.training_id %></td>
        <td><%= employee_training.tdate %></td>
        <td><%= link_to 'Show', employee_training %></td>
        <td><%= link_to 'Edit', edit_employee_training_path(employee_training) %></td>
        <td><%= link_to 'Destroy', employee_training, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'New Employee training', new_employee_training_path %>